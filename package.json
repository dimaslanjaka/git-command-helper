{
  "name": "git-command-helper",
  "version": "1.2.0",
  "description": "github command helper for nodejs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE*",
    "readme.md",
    "!**/*.tsbuildinfo",
    "!**/package-lock.json",
    "!**/yarn.lock",
    "!**/release/**",
    "!**/tmp/**"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.build.json && npm run pack",
    "pack": "node packer.js --yarn",
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
    "update:build-release": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/.github/workflows/build-release.yml > .github/workflows/build-release.yml",
    "update:typedoc": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/typedoc.js > typedoc.js",
    "update:typedoc-runner": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/typedoc-runner.js > typedoc-runner.js",
    "update:typedoc-server": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/typedoc-server.js > typedoc-server.js",
    "update:packer": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js",
    "update:ncu": "npx npm-check-updates -u -x chalk",
    "update": "run-s 'update:**'",
    "patch": "npm version patch",
    "pretest": "node -r ts-node/register test/pretest.ts",
    "test": "jest --runInBand --config=jest.config.ts",
    "test-cov": "nyc --reporter=lcovonly npm test",
    "docs": "gulp docs",
    "clean": "rm -rf dist",
    "deadcode": "ts-prune",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/git-command-helper.git"
  },
  "keywords": [
    "git",
    "gitmodules",
    "doctor",
    "command",
    "helper",
    "node-command-git",
    "github",
    "cli-parser",
    "git-cli-parser"
  ],
  "author": "Dimas Lanjaka <dimaslanjaka@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimaslanjaka/git-command-helper/issues"
  },
  "homepage": "https://www.webmanajemen.com/docs/git-command-helper/",
  "dependencies": {
    "@expo/spawn-async": "^1.7.2",
    "bluebird": "^3.7.2",
    "core-js": "^3.30.2",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/raw/private/release/cross-spawn.tgz",
    "deepmerge-ts": "^5.1.0",
    "fs-extra": "^11.1.1",
    "hexo-util": "^3.0.1",
    "ini": "^4.1.0",
    "sbg-utility": "^1.0.9",
    "true-case-path": "^2.2.1",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@jest/environment": "^29.5.0",
    "@jest/expect": "^29.5.0",
    "@jest/globals": "^29.5.0",
    "@jest/types": "^29.5.0",
    "@types/bluebird": "^3.5.38",
    "@types/core-js": "^2.5.5",
    "@types/git-command-helper": "https://github.com/dimaslanjaka/git-command-helper/raw/monorepo/release/git-command-helper.tgz",
    "@types/ini": "^1.3.31",
    "@types/minimist": "^1",
    "@types/node": "^20.1.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "ansi-colors": "^4.1.3",
    "axios": "^1.4.0",
    "axios-cache-interceptor": "^1.0.1",
    "binary-collections": "^1.0.3",
    "browser-sync": "^2.29.1",
    "debug": "^4.3.4",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "glob": "^10.2.2",
    "gulp": "^4.0.2",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "jest-config": "^29.5.0",
    "jest-mock": "^29.5.0",
    "jsonc-parser": "^3.2.0",
    "lint-staged": "^13.2.2",
    "minimist": "^1.2.8",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "semver": "^7.5.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-prune": "^0.10.3",
    "typedoc": "^0.24.7",
    "typedoc-plugin-missing-exports": "^2.0.0",
    "typescript": "^5.0.4"
  },
  "directories": {
    "test": "test",
    "lib": "dist"
  }
}
