{
  "name": "git-command-helper",
  "version": "2.0.2",
  "description": "github command helper for nodejs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "doc": "docs/git-command-helper",
    "test": "test",
    "lib": "dist",
    "bin": "bin"
  },
  "packageManager": "yarn@4.9.2",
  "files": [
    "dist",
    "bin",
    "!**/*.tsbuildinfo",
    "!**/package-lock.json",
    "!**/yarn.lock",
    "!**/release*/**",
    "!**/tmp/**",
    "!**/node_modules/**"
  ],
  "scripts": {
    "build-exports": "node index-builder.js",
    "build:tsc": "tsc -b tsconfig.build.json",
    "build": "run-s \"build:**\"",
    "pack": "node packer.js --yarn",
    "update:postinstall": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
    "update:build-release": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/.github/workflows/build-release.yml > .github/workflows/build-release.yml",
    "update:typedoc": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/typedoc.js > typedoc.config.js",
    "update:typedoc-runner": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/typedoc-runner.js > typedoc-runner.js",
    "update:typedoc-server": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/typedoc-server.js > typedoc-server.js",
    "update:packer": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/packer.js > packer.js",
    "update:ncu": "npx npm-check-updates -u -x chalk",
    "update:lockfile": "curl -L https://github.com/dimaslanjaka/git-command-helper/raw/master/yarn.lock > yarn.lock",
    "update": "run-s \"update:**\"",
    "patch": "npm version patch",
    "pretest": "npm run build && node -r ts-node/register test/pretest.ts",
    "test": "npx jest --runInBand --detectOpenHandles --logHeapUsage --update-snapshot --config=jest.config.ts",
    "test-cov": "npx nyc --reporter=lcovonly npm test --collect-coverage",
    "docs": "npm run clean && npm run build && gulp docs",
    "clean": "rimraf dist && npm run build",
    "deadcode": "ts-prune",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dimaslanjaka/git-command-helper.git"
  },
  "keywords": [
    "git",
    "gitmodules",
    "doctor",
    "command",
    "helper",
    "node-command-git",
    "github",
    "cli-parser",
    "git-cli-parser"
  ],
  "author": "Dimas Lanjaka <dimaslanjaka@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dimaslanjaka/git-command-helper/issues"
  },
  "homepage": "https://www.webmanajemen.com/docs/git-command-helper/",
  "dependencies": {
    "@expo/spawn-async": "^1.7.2",
    "binary-collections": "^2.0.6",
    "bluebird": "^3.7.2",
    "core-js": "^3.43.0",
    "cross-spawn": "*",
    "deepmerge-ts": "7",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.3",
    "ignore": "^7.0.5",
    "ini": "^5.0.0",
    "lodash": "^4.17.21",
    "minimatch": "^10.0.3",
    "sbg-utility": "^2.0.8",
    "upath": "^2.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.29.0",
    "@jest/environment": "^30.0.0",
    "@jest/expect": "^30.0.0",
    "@jest/globals": "^30.0.0",
    "@jest/types": "^30.0.0",
    "@types/bluebird": "^3",
    "@types/core-js": "^2.5.8",
    "@types/fs-extra": "^11",
    "@types/git-command-helper": "https://github.com/dimaslanjaka/git-command-helper/raw/9c813ce/release/git-command-helper.tgz",
    "@types/ini": "^4.1.1",
    "@types/lodash": "^4",
    "@types/minimist": "^1",
    "@types/node": "^24.0.3",
    "@types/which": "^3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "ansi-colors": "^4.1.3",
    "axios": "^1.10.0",
    "axios-cache-interceptor": "^1.8.0",
    "browser-sync": "^3.0.4",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.0",
    "globals": "^16.2.0",
    "gulp": "^5.0.1",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "jest-config": "^30.0.0",
    "jest-mock": "^30.0.0",
    "jsonc-parser": "^3.3.1",
    "lint-staged": "^16.1.2",
    "minimist": "^1.2.8",
    "nyc": "^17.1.0",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "semver": "^7.7.2",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "typedoc": "^0.28.5",
    "typedoc-plugin-missing-exports": "^4.0.0",
    "typescript": "^5.8.3"
  },
  "resolutions": {
    "binary-collections": "https://github.com/dimaslanjaka/bin/raw/463f3fe14fd732715e7637d38e34746473beccd6/releases/bin.tgz",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/raw/80999ac/release/cross-spawn.tgz",
    "ansi-regex": "^4",
    "glob-parent": "^6.0.2",
    "node.extend": "^2.0.2",
    "socket.io-parser": "^3.4.3",
    "unset-value": "^2.0.1"
  },
  "overrides": {
    "binary-collections": "https://github.com/dimaslanjaka/bin/raw/463f3fe14fd732715e7637d38e34746473beccd6/releases/bin.tgz",
    "cross-spawn": "https://github.com/dimaslanjaka/node-cross-spawn/raw/80999ac/release/cross-spawn.tgz",
    "ansi-regex": "^4",
    "glob-parent": "^6.0.2",
    "node.extend": "^2.0.2",
    "socket.io-parser": "^3.4.3",
    "unset-value": "^2.0.1"
  },
  "engines": {
    "node": ">=14",
    "npm": "^8"
  }
}
