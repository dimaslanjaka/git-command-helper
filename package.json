{
	"name": "git-command-helper",
	"version": "1.0.13",
	"description": "github command helper for nodejs",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"typings",
		"src",
		"LICENSE",
		"README.md",
		"*.js"
	],
	"scripts": {
		"build": "tsc -p tsconfig.build.json",
		"postbuild": "node packer.js",
		"preupdate": "curl -L https://github.com/dimaslanjaka/nodejs-package-types/raw/main/postinstall.js > postinstall.js",
		"update": "npx npm-check-updates -u -x chalk && npm install --cache tmp",
		"patch": "npm version patch",
		"postinstall": "node postinstall.js --commit"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dimaslanjaka/git-command-helper.git"
	},
	"keywords": [
		"git",
		"gitmodules",
		"doctor",
		"command",
		"helper",
		"node-command-git",
		"github",
		"cli-parser",
		"git-cli-parser"
	],
	"author": "Dimas Lanjaka <dimaslanjaka@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/dimaslanjaka/git-command-helper/issues"
	},
	"homepage": "https://github.com/dimaslanjaka/git-command-helper#readme",
	"dependencies": {
		"@expo/spawn-async": "^1.7.0",
		"bluebird": "^3.7.2",
		"cross-spawn": "^7.0.3",
		"deepmerge-ts": "^4.2.2",
		"fs-extra": "^11.1.0",
		"hexo-util": "^3.0.1",
		"ini": "^3.0.1",
		"true-case-path": "^2.2.1",
		"upath": "^2.0.1"
	},
	"devDependencies": {
		"@types/bluebird": "^3.5.38",
		"@types/cross-spawn": "^6.0.2",
		"@types/hexo-util": "^0.6.5",
		"@types/ini": "^1.3.31",
		"@types/node": "^18.11.17",
		"@typescript-eslint/eslint-plugin": "^5.47.0",
		"@typescript-eslint/parser": "^5.47.0",
		"debug": "^4.3.4",
		"eslint": "^8.30.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"gulp": "^4.0.2",
		"nodejs-package-types": "https://github.com/dimaslanjaka/nodejs-package-types/raw/main/release/nodejs-package-types.tgz",
		"prettier": "^2.8.1",
		"rimraf": "^3.0.2"
	}
}
